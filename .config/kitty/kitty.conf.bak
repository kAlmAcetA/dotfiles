# BEGIN_KITTY_THEME
# Hybrid
include current-theme.conf
# END_KITTY_THEME
#
font_family MesloLGS Nerd Font Mono
font_size 13

enable_audio_bell no

cursor_shape block
cursor_shape_unfocused hollow
# cursor #cccccc
shell_integration  no-cursor

scrollback_lines 10000
scrollback_pager_history_size 2048
